<html>
        <head>
            <title>'account wise report'</title>
        </head>
        <body>
                <section>
                          <form method='POST' action='acc_wise_report'>
                            <div class="form-group">
                              <label for="account_no">Account number</label>
                              <input type="text" name="account_no" id="account_no" required />
                              <label for="sdate">Select starting date</label>
                              <input type="date" name="sdate" id="sdate" required />
                              <label for="edate">Select ending date</label>
                              <input type="date" name="edate" id="edate" required />
                            </div>
                          </form>
                </section>
        </body>
</html>